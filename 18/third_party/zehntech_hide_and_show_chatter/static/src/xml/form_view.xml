<templates xml:space="preserve">
    <t t-inherit="web.ControlPanel" t-inherit-mode="extension">
        <xpath expr="//div[contains(@class, 'o_control_panel_navigation')]" position="inside">
            <button type="button" class="btn btn-primary history_btn d-none" id="history_btn" title="Show Chatter" t-on-click="toggleChattershow"  >
                <i class="fa fa-eye"></i>
            </button>
        </xpath>
    </t>
</templates>        
 