<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--Inventory History Wizard views-->
    <data>
        <record id="vtt_view_form_inventory_history_wz" model="ir.ui.view">
            <field name="name">Inventory Report at Date</field>
            <field name="model">vtt.inventory.history.wz</field>
            <field name="arch" type="xml">
                <form string="Choose your date">
                    <group>
                        <group>
                            <field name="date_from"/>
                        </group>
                        <group>
                            <field name="date_to"/>
                        </group>
                    </group>
                    <footer>
                        <button name="open_at_date" string="Confirm" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>