<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Request Sequence -->
        <record id="seq_vtt_official_dispatch_request" model="ir.sequence">
            <field name="name">Oficial Dispatch Request</field>
            <field name="code">vtt.official.dispatch.request</field>
            <field name="prefix">ODR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Document Sequence -->
        <record id="seq_vtt_official_dispatch_document" model="ir.sequence">
            <field name="name">Oficial Dispatch Document</field>
            <field name="code">vtt.official.dispatch.document</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
