<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="vtt_seq_repair_order" model="ir.sequence">
            <field name="name">Repair Order</field>
            <field name="code">repair.order</field>
            <field name="prefix">RO</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="vtt_seq_repair_diagnosis" model="ir.sequence">
            <field name="name">Repair Diagnosis</field>
            <field name="code">repair.diagnosis</field>
            <field name="prefix">ES</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="vtt_seq_maintenance_plan" model="ir.sequence">
            <field name="name">Maintenance Plan</field>
            <field name="code">maintenance.plan</field>
            <field name="prefix">MP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

</odoo>
