<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--Custom Rating Stage data-->
    <data noupdate="1">
        <record id="vtt_custom_rating_stage_new" model="vtt.custom.rating.stage">
            <field name="name">New</field>
            <field name="type">public</field>
            <field name="sequence">1</field>
        </record>

        <record id="vtt_custom_rating_stage_received" model="vtt.custom.rating.stage">
            <field name="name">Received</field>
            <field name="type">public</field>
            <field name="sequence">2</field>
        </record>

        <record id="vtt_custom_rating_stage_progress" model="vtt.custom.rating.stage">
            <field name="name">In Progress</field>
            <field name="type">public</field>
            <field name="sequence">3</field>
        </record>

        <record id="vtt_custom_rating_stage_done" model="vtt.custom.rating.stage">
            <field name="name">Done</field>
            <field name="type">public</field>
            <field name="sequence">4</field>
            <field name="fold" eval="True"/>
            <field name="is_closed" eval="True"/>
        </record>
    </data>
</odoo>