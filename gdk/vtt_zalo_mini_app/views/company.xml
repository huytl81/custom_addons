<odoo>
    <!-- Company views -->
  <data>

    <record id="zalo_res_company_inherit_form" model="ir.ui.view">
        <field name="name">Zao - Inherit company form</field>
      <field name="model">res.company</field>
      <field name="inherit_id" ref="base.view_company_form"/>
<!--      <field name="mode">form</field>-->
      <field name="arch" type="xml">
          <field name="currency_id" position="after">
              <field name="zalo_app_qr" widget="image"/>
              <field name="bank_qr" widget="image"/>
              <field name="zalo_app_link" string="Zalo app link"/>
              <field name="customer_support_ids" string="NV CSKH" widget="many2many_tags"/>
              <field name="customer_support_partner_ids" string="NV CSKH ids"/>
              <field name="sale_discuss_channel_id"/>
          </field>
      </field>
    </record>

    <!-- <record id="view_res_company_form_inherit" model="ir.ui.view">
