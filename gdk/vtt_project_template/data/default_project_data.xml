<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--Default Task Type data-->
    <data noupdate="1">
        <record id="vtt_data_project_task_type_new" model="project.task.type">
            <field name="name">New</field>
            <field name="fold">False</field>
            <field name="sequence">0</field>
            <field name="user_id" eval="False"/>
        </record>

        <record id="vtt_data_project_task_type_progress" model="project.task.type">
            <field name="name">Progress</field>
            <field name="fold">False</field>
            <field name="sequence">10</field>
            <field name="user_id" eval="False"/>
        </record>

        <record id="vtt_data_project_task_type_done" model="project.task.type">
            <field name="name">Done</field>
            <field name="fold">False</field>
            <field name="sequence">20</field>
            <field name="user_id" eval="False"/>
        </record>

        <record id="vtt_data_project_task_type_cancel" model="project.task.type">
            <field name="name">Cancel</field>
            <field name="fold">True</field>
            <field name="sequence">25</field>
            <field name="user_id" eval="False"/>
        </record>
    </data>
</odoo>