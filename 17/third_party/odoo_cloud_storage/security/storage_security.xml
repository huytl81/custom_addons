<odoo>
<data noupdate="0">
	<record model="ir.module.category" id="module_cloud_storage_odoo">
		<field name="name">Cloud Odoo Storage</field>
		<field name="description">Cloud Integration Applilcation</field>
		<field name="sequence">20</field>
	</record>

	<record id="group_cloud_user" model="res.groups">
		<field name="name">User</field>
		<field name="comment">User will have read access to the Cloud Storage Module.</field>
		<field name="category_id" ref="module_cloud_storage_odoo"/>
	</record>

	<record id="group_cloud_manager" model="res.groups">
		<field name="name">Manager</field>
		<field name="comment">Full Access on Cloud Module</field>
		<field name="category_id" ref="module_cloud_storage_odoo"/>
		<field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
		 <field name="implied_ids" eval="[(4, ref('group_cloud_user'))]"/>
	</record>
</data>
</odoo>
