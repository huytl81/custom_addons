<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--Task Duplicate Wizard views-->
    <data>
        <!--Form view-->
        <record id="vtt_view_form_project_task_duplicate_wz" model="ir.ui.view">
            <field name="name">Task Duplicate Wizard</field>
            <field name="model">vtt.project.task.duplicate.wz</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <notebook name="main_book">
                            <page string="Options">
                                <group>
                                    <group></group>
                                    <group>
                                        <field name="keep_assign" widget="boolean_toggle"/>
                                    </group>
                                </group>
                            </page>

                            <page string="Tasks">
                                <field name="task_ids"/>
                            </page>
                        </notebook>

                        <notebook name="project_book">
                            <page string="Projects">
                                <field name="project_ids"/>
                            </page>
                        </notebook>
                    </sheet>

                    <footer>
                        <button name="dup_task_2_project" string="Apply" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>