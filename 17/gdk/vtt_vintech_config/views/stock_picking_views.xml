<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--Stock Picking views-->
    <data>
        <!--Kanban view add Partner Information-->
        <record id="vtt_view_form_stock_picking_partner_info" model="ir.ui.view">
            <field name="name">Stock Picking Form add Partner Information</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <field name="partner_id" position="after">
                    <field name="partner_vat"/>
                    <field name="partner_street"/>
                    <field name="vintech_street_note"/>
                </field>
            </field>
        </record>
    </data>
</odoo>