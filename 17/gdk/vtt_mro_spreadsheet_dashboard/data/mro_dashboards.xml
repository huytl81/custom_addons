<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--MRO Dashboard-->
    <data>
        <record id="vtt_spreadsheet_dashboard_group_mro" model="spreadsheet.dashboard.group">
            <field name="name">MRO</field>
            <field name="sequence">200</field>
        </record>

        <record id="vtt_spreadsheet_dashboard_mro_repair_order" model="spreadsheet.dashboard">
            <field name="name">Repairs</field>
            <field name="spreadsheet_binary_data" type="base64" file="vtt_mro_spreadsheet_dashboard/data/files/vtt_mro_dashboard.osheet.json"/>
            <field name="dashboard_group_id" ref="vtt_spreadsheet_dashboard_group_mro"/>
            <field name="group_ids" eval="[Command.link(ref('vtt_mro.vtt_group_mro_user'))]"/>
            <field name="sequence">100</field>
        </record>
    </data>
</odoo>
