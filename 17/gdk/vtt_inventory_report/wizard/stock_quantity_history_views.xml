<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--Inventory History Wizard views-->
    <data>
        <record id="vtt_view_form_stock_quantity_history_warehouse" model="ir.ui.view">
            <field name="name">Quantity History Form add Warehouse</field>
            <field name="model">stock.quantity.history</field>
            <field name="inherit_id" ref="stock.view_stock_quantity_history"/>
            <field name="arch" type="xml">
                <field name="inventory_datetime" position="after">
                    <field name="warehouse_id"/>
                </field>
            </field>
        </record>
    </data>
</odoo>