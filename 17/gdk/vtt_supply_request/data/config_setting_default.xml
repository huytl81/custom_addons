<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <function model="stock.picking.type" name="write">
            <value model="stock.picking.type" eval="obj().env['stock.warehouse'].search([]).int_type_id.ids"/>
            <value eval="{'active': True}"/>
        </function>

        <record id="stock.stock_location_view_tree2_editable" model="ir.ui.view">
            <field name="active" eval="False"/>
        </record>

        <record id="stock.stock_location_view_form_editable" model="ir.ui.view">
            <field name="active" eval="False"/>
        </record>
    </data>
</odoo>